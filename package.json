{
  "name": "pixelboost",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "build:sass": "sass --no-source-map --style=compressed src/assets/sass:./public/assets/css",
    "watch:sass": "sass --source-map --watch src/assets/sass:./public/assets/css",
    "watch:cms": "npx netlify-cms-proxy-server",
    "start": "del-cli ./public --force && npm-run-all --parallel watch:*",
    "build": "npm-run-all --parallel build:*"
  },
  "keywords": [],
  "author": "Josh Del Vecchio",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5"
  },
  "dependencies": {
    "del-cli": "^5.0.0",
    "netlify-cms-proxy-server": "^1.3.24",
    "npm-run-all": "^4.1.5",
    "sass": "^1.63.3"
  }
}
